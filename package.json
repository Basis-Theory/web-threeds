{
  "name": "@basis-theory/3ds-web",
  "version": "1.0.0",
  "repository": "https://github.com/Basis-Theory/3ds-web",
  "license": "Apache-2.0",
  "author": "Basis Theory <support@basistheory.com>",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "prettier": "@basis-theory/eslint-config/prettier",
  "scripts": {
    "prepare": "husky install",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "clean": "rimraf dist",
    "build": " swc ./src --copy-files -d dist && tsc --emitDeclarationOnly",
    "build:dev": "yarn clean && yarn build",
    "build:watch": "swc ./src -d dist -w && tsc --emitDeclarationOnly",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "prettier": "prettier '**/*.{ts,json}'",
    "prettier:write": "prettier --write '**/*.{ts,json}'",
    "type-check": "tsc --noEmit",
    "lint-staged": "lint-staged",
    "release": "semantic-release",
    "bundle": "unbuild --minify",
    "bundle:dev": "yarn clean && yarn bundle"
  },
  "dependencies": {},
  "devDependencies": {
    "@basis-theory/eslint-config": "^1.2.0",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/npm": "^11.0.2",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@size-limit/preset-small-lib": "^11.0.1",
    "@swc/cli": "0.1.63",
    "@swc/core": "^1.3.75",
    "@swc/jest": "^0.2.28",
    "@types/jest": "^29.5.11",
    "@types/uuid": "^9.0.7",
    "eslint": "^8.56.0",
    "husky": "^8.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^3.1.1",
    "pretty-quick": "^3.1.3",
    "rimraf": "^5.0.5",
    "semantic-release": "^22.0.12",
    "size-limit": "^11.0.1",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "uuid": "^9.0.1"
  },
  "resolutions": {},
  "engines": {
    "node": ">=10.12.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "size-limit": [
    {
      "path": "dist/3ds-web.bundle.js",
      "limit": "50 KB"
    }
  ],
  "private": false
}
